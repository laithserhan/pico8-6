pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
dirs={}
dirs.left=0
dirs.right=1

kinds={}
kinds.player=1

btns={}
btns.left=0
btns.right=1
btns.jump=4

cam={}
cam.x=0
cam.y=0

friction=0.75
gravity=0.08
accel=0.09

function is_solid(x,y)
 if (x < 0 or x >= 128) then
  return true
 end
 local tile = mget(x, y)
 return fget(tile,0)
end

function create_actor(kind,x,y,frame)
 local a={}
 a.kind=kind
 a.x=x
 a.y=y
 a.dx=0
 a.dy=0
 a.onground=false
 a.frame=frame
 add(actors, a)
 return a
end

function create_player(x,y,frame)
 local pl = create_actor(kinds.player,x,y,frame)
 pl.dir=dirs.right
 return pl 
end

function move_actor(a)
 if (a.kind == kinds.player) then
  move_player(a)
 end

 a.onground=false
 
 -- move horizontal
 local x1 = a.x + a.dx + sgn(a.dx) * 0.3
 if (is_solid(x1, a.y-0.5)) then
  -- obstruction in the way
  -- move as close as possible
  while (not is_solid(a.x + sgn(a.dx)*0.3, a.y-0.5)) do
   a.x += sgn(a.dx) * 0.1
  end
 else
  -- all clear, move on
  a.x += a.dx
 end
 
 -- vertical
 if (a.dy < 0) then
  -- up
  if (is_solid(a.x-0.2, a.y+a.dy-1) or
      is_solid(a.x+0.2, a.y+a.dy-1)) then
   -- obstruction in the way
   -- move as close as possible
   a.dy = 0
   while (not (is_solid(a.x-0.2, a.y-1) or
               is_solid(a.x+0.2, a.y-1))) do
    a.y -= 0.01
   end
  else
   -- all clear, move up
   a.y += a.dy     
  end
 else
  --down
  if (is_solid(a.x-0.2, a.y+a.dy) or
      is_solid(a.x+0.2, a.y+a.dy)) then
   -- obstruction in the way
   -- move as close as possible
   a.dy = 0
   a.onground = true
   -- snap down
   while (not (is_solid(a.x-0.2, a.y) or
               is_solid(a.x+0.2, a.y))) do
    a.y += 0.05
   end
  else
   a.y += a.dy
  end
 end
 
 a.dy += gravity
 a.dy *= 0.95
 
 -- friction
 if (a.onground) then
  a.dx *= friction
 else
  a.dx *= friction
 end
end

function move_player(a)
	if btn(btns.left) then
  a.dx -= accel
	 a.dir = dirs.left
	end
	
	if btn(btns.right) then
  a.dx += accel
  a.dir = dirs.right
	end

 if btnp(btns.jump) and a.onground then
  a.dy = -0.7
 end

	if a.onground then
	 a.frame = 64
	else
	 a.frame = 65
	end
end

function draw_actor(a)
 flipx=a.dir==dirs.left
 spr(a.frame, a.x*8-4, a.y*8-4, 1, 1, flipx)
end

function update_camera(player)
	cam.x = mid(0, player.x*8-64, 256-128)
	camera(cam.x, cam.y)
end

function draw_map()
	mapdraw(0,0,0,0,128,64)
end

function _init()
 actors = {}
 player = create_player(1, 0, 64)
end

function _update()
 foreach(actors, move_actor)
end

function _draw()
 camera(0,0)
 rectfill(0,0, 127, 127, 0)

 update_camera(player) 
 draw_map()
	foreach(actors, draw_actor)
end

